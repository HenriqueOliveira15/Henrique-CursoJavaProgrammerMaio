	public static void main(String[] args) {
		
		Cachorro objetoCachorro = new Cachorro ();
		
		Scanner entradaDados = new Scanner (System.in);
		
		System.out.println("Digite o Nome do cachorro: ");
		objetoCachorro.nome  = entradaDados.next();
		
;
		
		

	}

}
